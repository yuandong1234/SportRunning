<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountDownProgress">

        <!-- 圆实心的颜色 -->
        <attr name="circSolidColor" format="color" />

        <!-- 圆边框的颜色 -->
        <attr name="circFrameColor" format="color" />

        <!-- 进度条的颜色 -->
        <attr name="progressColor" format="color" />

        <!-- 文字的颜色 -->
        <attr name="ProgressText_color" format="color" />

    </declare-styleable>

    <declare-styleable name="SportProgressView">
        <attr name="sportProgressView_color" format="color" />        <!--进度圆环颜色-->
        <attr name="sportProgressView_stroke_width" format="dimension" />        <!--进度圆环宽度-->
        <attr name="sportProgressView_progress" format="integer" />       <!--圆环进度-->
    </declare-styleable>

</resources>